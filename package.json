{
  "name": "magical-domains",
  "version": "1.0.0",
  "description": "AI-Generated Websites in Seconds",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "vercel-build": "tsc && mkdir -p public && cp index.html public/ && cp -r dist public/",
    "dev": "tsc --watch",
    "start": "python -m http.server 8000",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "website-generator",
    "domains",
    "typescript"
  ],
  "author": "Magical Domains",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "files": [
    "dist/**/*",
    "public/**/*",
    "index.html",
    "README.md"
  ]
} 